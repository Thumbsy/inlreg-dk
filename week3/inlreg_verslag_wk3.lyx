\begin_layout Enumerate
\noindent
Bereken de stapresponsie y(t).
 Laat voor het gemak eerst K* en τ* in de afleiding staan en vul op het
 eind van de afleiding K in bij K* en τ*
\end_layout

\begin_deeper
\begin_layout Standard
\noindent
\begin_inset Formula $Y(s)=X(s)\times H(s)=\frac{1}{s}\times\frac{K^{*}}{\tau^{*}s+1}=\frac{K^{*}}{s(\tau^{*}s+1)}=\frac{A}{s}+\frac{B}{\tau^{*}s+1}\Leftrightarrow$
\end_inset


\end_layout

\begin_layout Standard
\noindent
\begin_inset Formula $K^{*}=A(\tau^{*}s+1)+B\times s$
\end_inset


\end_layout

\begin_layout Standard
\noindent
Stel 
\begin_inset Formula $s=0$
\end_inset

:
\end_layout

\begin_layout Standard
\noindent
\begin_inset Formula $K^{*}=A(1)+0\Leftrightarrow A=K^{*}$
\end_inset


\end_layout

\begin_layout Standard
\noindent
Stel 
\begin_inset Formula $s=\frac{-1}{\tau^{*}}$
\end_inset

:
\end_layout

\begin_layout Standard
\noindent
\begin_inset Formula $K^{*}=0+B\times\frac{-1}{\tau^{*}}\Leftrightarrow B=-K^{*}\times\tau^{*}$
\end_inset


\end_layout

\begin_layout Standard
\noindent
\begin_inset Formula $Y(s)=K^{*}\frac{1}{s}-K^{*}\tau^{*}\frac{1}{\tau^{*}s+1}=K^{*}\frac{1}{s}-K^{*}\tau^{*}\frac{\frac{1}{\tau*}}{s+\frac{1}{\tau*}}$
\end_inset


\begin_inset Newline newline
\end_inset


\begin_inset Formula $y(t)=K^{*}-K^{*}\tau^{*}\frac{1}{\tau^{*}}e^{-\frac{1}{\tau^{*}}t}=K^{*}(1-e^{-\frac{1}{\tau^{*}}t})=\frac{K}{1+K}(1-e^{-\frac{1}{\frac{\tau}{1+K}}t})$
\end_inset


\begin_inset Newline newline
\end_inset


\end_layout

\end_deeper
