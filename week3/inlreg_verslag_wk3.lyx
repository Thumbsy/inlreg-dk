\end_layout

\begin_layout Standard
\noindent
\begin_inset Formula $Y(s)=H_{1}(s)(X(s)-Y(s))\Leftrightarrow Y(s)(1+H_{1}(s))=X(s)H(s)\Leftrightarrow$
\end_inset


\begin_inset Newline newline
\end_inset


\begin_inset Formula $H(s)=\frac{Y(s)}{X(s)}=\frac{H_{1}(s)}{1+H_{1}(s)}=\frac{1}{\frac{1}{H_{1}(s)}+1}=\frac{1}{\frac{\tau s+1}{K}+1}=\frac{K}{\tau s+1+K}=\frac{\frac{K}{1+K}}{\frac{\tau}{1+K}s+1}=\frac{K^{*}}{\tau^{*}s+1}$
\end_inset

, met 
\begin_inset Formula $K*$
\end_inset

=
\begin_inset Formula $\frac{K}{1+K}$
\end_inset

 en 
\begin_inset Formula $\tau^{*}=\frac{\tau}{1+K}$
\end_inset

.
